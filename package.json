{
	"name": "pkg-to-jsr",
	"type": "module",
	"version": "0.1.6",
	"packageManager": "pnpm@9.7.0+sha512.dc09430156b427f5ecfc79888899e1c39d2d690f004be70e05230b72cb173d96839587545d09429b55ac3c429c801b4dc3c0e002f653830a420fa2dd4e3cf9cf",
	"description": "zero-config generator that creates a jsr.json file from your existing package.json",
	"author": "ryoppippi",
	"license": "MIT",
	"repository": "ryoppippi/pkg-to-jsr",
	"keywords": [
		"jsr",
		"package.json",
		"cli",
		"zero-config"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"bin": {
		"pkg-to-jsr": "./bin/pkg-to-jsr.mjs"
	},
	"files": [
		"bin",
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"typecheck": "tsc --noEmit",
		"release": "nr typecheck && nr test run && nr build && nlx publint && nlx bumpp && npm publish",
		"lint": "eslint --cache .",
		"format": "nr lint --fix",
		"test": "vitest --typecheck",
		"prepare": "ts-patch install && typia patch && ./scripts/gen_types.js"
	},
	"dependencies": {
		"citty": "^0.1.6",
		"consola": "^3.2.3",
		"pathe": "^1.1.2"
	},
	"devDependencies": {
		"@antfu/ni": "^0.22.0",
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config@^0.0.21",
		"@ryoppippi/unplugin-typia": "npm:@jsr/ryoppippi__unplugin-typia",
		"@types/bun": "^1.1.6",
		"dax-sh": "^0.41.0",
		"eslint": "^9.8.0",
		"eslint-plugin-format": "^0.1.2",
		"json-schema-to-typescript": "^15.0.0",
		"pkg-types": "^1.1.3",
		"ts-patch": "^3.2.1",
		"tsdown": "^0.2.4",
		"typescript": "^5.5.4",
		"typia": "^6.7.2",
		"vitest": "^2.0.5"
	}
}
