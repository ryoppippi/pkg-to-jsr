{
	"name": "pkg-to-jsr",
	"type": "module",
	"version": "1.1.1",
	"packageManager": "pnpm@9.7.1",
	"description": "zero-config generator that creates a jsr.json file from your existing package.json",
	"author": "ryoppippi",
	"license": "MIT",
	"repository": "ryoppippi/pkg-to-jsr",
	"keywords": [
		"jsr",
		"package.json",
		"cli",
		"zero-config"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"bin": {
		"pkg-to-jsr": "./bin/pkg-to-jsr.mjs"
	},
	"files": [
		"bin",
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"cli": "./bin/pkg-to-jsr.mjs",
		"typecheck": "tsc --noEmit",
		"release": "nr typecheck && nr test run && nr build && nlx publint && nlx bumpp && npm publish",
		"lint": "eslint --cache .",
		"format": "nr lint --fix",
		"test": "vitest --typecheck",
		"prepare": "ts-patch install && typia patch && ./scripts/gen_types.js",
		"prepublishOnly": "nr build"
	},
	"dependencies": {
		"cleye": "^1.3.2",
		"consola": "^3.2.3"
	},
	"devDependencies": {
		"@antfu/ni": "^0.22.4",
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config@^0.0.22",
		"@ryoppippi/unplugin-typia": "npm:@jsr/ryoppippi__unplugin-typia",
		"@types/bun": "^1.1.6",
		"dax-sh": "^0.41.0",
		"eslint": "^9.9.0",
		"eslint-plugin-format": "^0.1.2",
		"find-up-simple": "^1.0.0",
		"json-schema-to-typescript": "^15.0.0",
		"pathe": "^1.1.2",
		"pkg-types": "^1.1.3",
		"ts-patch": "^3.2.1",
		"tsdown": "^0.2.6",
		"typescript": "^5.5.4",
		"typia": "^6.8.0",
		"vite-plugin-doctest": "^1.0.0",
		"vitest": "^2.0.5"
	}
}
